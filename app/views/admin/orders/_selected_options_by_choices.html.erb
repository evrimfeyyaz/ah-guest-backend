<% if selected_options.any? %>
    <ol>
      <% selected_options.group_by { |option| option.choice.title }.each do |choice_title, options| %>
          <li>
            <strong><%= choice_title %>:</strong>
            <% options.each do |option| %>
                <ol>
                  <li><%= option.title %></li>
                </ol>
            <% end %>
          </li>
      <% end %>
    </ol>
<% end %>